<form action ="/order/update/<%= order.id %>" method = "POST" class="form-signin">
	<h2> Editing order </h2>

	<input value="<%= order.restaurant %>" name="restaurant" type="text" class="form-control" />
	<input value="<%= order.orderDescription %>" name="orderDescription" type="text" class="form-control" />
	
	<input type="submit" value="Continue" class= "btn btn-lg btn-primary btn-block" />
	<input type="hidden" name="_csrf" value="<%= _csrf %>" />
</form>
